{
	"bracketSpacing": false,
	"htmlWhitespaceSensitivity": "css",
	"jsxBracketSameLine": true,
	"overrides": [
		{
			"files": "*.test.js",
			"options": {
				"semi": true
			}
		},
		{
			"files": ["*.html", "legacy/**/*.js"],
			"options": {
				"tabWidth": 4
			}
		}
	],
	"printWidth": 140,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 4,
	"trailingComma": "es5",
	"useTabs": true
}
